{
    "manifest_version": 3,
    "name": "Plasmo Sidebar",
    "version": "1.0",
    "permissions": ["storage", "activeTab", "tabs", "scripting"],
    "host_permissions": ["<all_urls>"],
    "sidebar": {
      "default_panel": "sidebar.html"
    },
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html"
    }
  }
  